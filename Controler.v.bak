module Controler(clk,instruction,ALUop,RegWrite);
  
  input clk;
  input [31:0] instruction;
  wire [6:0] ins;
  output reg RegWrite;
  output reg [1:0] ALUop;
  
  
  assign ins = instruction[6:0];
  
  
  always@(posedge clk)
  case(ins)
    
    51 :begin
         ALUop = 2;
         RegWrite = 1;
        end 
    3 : begin
        ALUop = 0;
        RegWrite = 1;
        end
        
    35 :begin
        ALUop = 0;
        RegWrite = 0;
        end
        
    99 :begin
         ALUop = 1;
         RegWrite = 0;
        end
  endcase
    
  
endmodule